<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bishe2.bishe2.mapper.LoginMapper">
    <select id="find" parameterType="java.lang.String" resultType="java.lang.String">
            select user_password from userinfo where user_id =#{username}
    </select>
    <insert id="add" parameterType="java.util.Map">
        insert into userinfo (user_id,user_password)
        values (#{id},#{password})
    </insert>
</mapper>
